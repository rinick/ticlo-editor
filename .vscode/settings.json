// Place your settings in this file to overwrite default and user settings.
{
    "files.associations": {
        "*.vue": "vue"
    },
    "files.exclude": {
        "**/.git": true,
        "**/*.js.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "files.eol": "\n",
    "editor.tabSize": 2,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false
}
